{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/join-group",
        "destination": "/join-group.html"
      },
      {
        "source": "/members",
        "destination": "/members.html"
      },
      {
        "source": "**",
        "destination": "index.html"
      }
    ]
  }
}
